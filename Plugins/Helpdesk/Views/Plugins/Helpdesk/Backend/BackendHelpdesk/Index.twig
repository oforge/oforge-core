                                    <td class="mailbox-star"><a href="#"><i class="fa fa-circle text-red"></i></a>
                                    </td>
                                    <td class="mailbox-date">5 mins ago</td>
                                </tr>
                                </tbody>
                            </table>
                            <!-- /.table -->
                        </div>
                        <!-- /.mail-box-messages -->
                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer no-padding">
                        <div class="mailbox-controls">
